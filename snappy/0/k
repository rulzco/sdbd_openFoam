FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0003375;

boundaryField
{
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.0003375;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.0003375;
        value           uniform 0.0003375;
    }
    airfoil
    {
        type            fixedValue;
        value           uniform 1e-10;
    }
}


