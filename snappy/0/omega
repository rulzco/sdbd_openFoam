FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.07348;

boundaryField
{
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.07348;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.07348;
        value           uniform 0.07348;
    }
    airfoil
    {
        type            omegaWallFunction;
        value           uniform 1000;
    }
}


// ************************************************************************* //
